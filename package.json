    "@types/xterm": "^3.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "source-map-support": "^0.5.12",
    "xterm": "^4.1.0"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "^1.9.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
